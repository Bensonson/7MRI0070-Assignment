    </joint>
    <joint name="pivot4_to_arm4" type="revolute">
        <parent link="pivot4" />
        <child link="arm4" />
        <axis xyz="1 0 0" />
        <limit lower="-1.57" upper="1.57" effort="30" velocity="1" />
    </joint>
    <joint name="arm4_to_end_effector" type="fixed">
        <origin xyz="0 0 0.5" rpy="0 0 0" />
        <parent link="arm4" />
        <child link="end_effector" />
    </joint>
    <joint name="inject_joint" type="prismatic">
        <origin xyz="0 0 3.5" rpy="0 0 0" />
        <parent link="end_effector" />
        <child link="needle" />
        <axis xyz="0 0 1" />
        <limit effort="30" velocity="1.0" lower="0" upper="5" />
    </joint>
    <transmission name="trans_base_link_to_rotator1">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="base_link_to_rotator1">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="base_link_to_rotator1_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
