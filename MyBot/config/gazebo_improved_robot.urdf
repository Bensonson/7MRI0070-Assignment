            <geometry>
                <box size="10 10 1" />
            </geometry>
            <material name="brown" />
        </visual>
        <collision>
            <geometry>
                <box size="10 10 1" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>
    </link>
    <link name="rotator1">
